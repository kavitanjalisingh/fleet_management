<?xml version="1.0"?>
<tryton>
    <data>

        <record model="ir.ui.view" id="fleet_management_product">
            <field name="model">product.template</field>
            <field name="inherit" ref="product.template_view_form" />
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath expr="//field[@name='list_price']" position="after">
                        <label name="fleet_management_type" />
                        <field name="fleet_management_type" />
                    </xpath>
                </data>
                ]]>
            </field>
        </record>

    </data>
</tryton>
